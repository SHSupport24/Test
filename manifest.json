    "icon": {
      "url": "https://test-iota-self-48.vercel.app/icon.png",
      "url2x": "https://test-iota-self-48.vercel.app/icon@2x.png"
    },
    "iframeConnectorUrl": "https://test-iota-self-48.vercel.app/",
    "id": "trello-tracking-powerup"
  }